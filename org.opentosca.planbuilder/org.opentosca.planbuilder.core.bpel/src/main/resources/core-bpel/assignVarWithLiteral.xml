<bpel:assign xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable" validate="no" name="AssignVarWihtLiteral">
<!-- Assign Variable with literal:
	Literal = $literal
	VarName = $VarName
	Intent = $intent
 -->
	<bpel:copy>
		<bpel:from>
			<bpel:literal>
				$literal
			</bpel:literal>
		</bpel:from>
		<bpel:to variable="$VarName"></bpel:to>
	</bpel:copy>
</bpel:assign>
